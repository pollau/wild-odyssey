{
    "navigationFallback": {
        "rewrite": "/keystatic/index.html",
        "exclude": [
            "/assets/*",
            "/images/*",
            "*.css",
            "*.js",
            "*.ico"
        ]
    },
    "routes": [
        {
            "route": "/keystatic/*",
            "rewrite": "/keystatic/index.html"
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/404.html"
        }
    },
    "mimeTypes": {
        ".json": "application/json"
    }
}